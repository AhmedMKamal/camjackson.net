{
  "name": "camjackson.net",
  "version": "0.0.1",
  "description": "My personal website, including blogging with markdown",
  "engines": {
    "node": "0.12.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/camjackson/camjackson.net.git"
  },
  "author": "Cam Jackson",
  "license": "GPL-3.0",
  "main": "server.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src -d lib",
    "build-site": "babel-node src/buildSite.js",
    "dev": "LOCAL_DB=1 nodemon --exec ./node_modules/.bin/babel-node -- src/app.js",
    "deploy": "npm run package && aws lambda update-function-code --function-name views-index --zip-file fileb://lambda.zip",
    "init": "scripts/init",
    "package": "npm run build && rm lambda.zip; zip -dd -q -r lambda.zip node_modules lib/*",
    "prestart": "npm run build",
    "seed": "scripts/seed",
    "start": "node lib/app.js",
    "test": "NO_LOGGING=1 ./node_modules/.bin/mocha --require babel-core/register --reporter min --watch --recursive test",
    "test-single": "NO_LOGGING=1 ./node_modules/.bin/mocha --require babel-core/register --recursive test"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.0.15",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.0.15",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.1",
    "bunyan": "^1.5.1",
    "connect-dynamodb": "^1.0.6",
    "dynasty": "^0.2.4",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "helmet": "^0.15.0",
    "highlight.js": "^8.9.1",
    "install": "^0.4.1",
    "kerberos": "0.0.17",
    "marked": "^0.3.5",
    "method-override": "^2.3.5",
    "moment": "^2.10.6",
    "npm": "^3.5.2",
    "passport": "^0.3.0",
    "passport-google-oauth": "^0.2.0",
    "passport-local": "^1.0.0",
    "prompt": "~0.2.14",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "xml": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "chai": "^3.4.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "q": "^1.4.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.2"
  }
}
